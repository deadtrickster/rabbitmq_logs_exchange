{application, rabbitmq_logs_exchange,
 [{description, "RabbitMQ Logs Exchange"},
  {vsn, "0.3.0"},
  {modules, []},
  {registered, []},
  {env, [{mongodb_pool,
          {logs_exchange_mongodb_pool,
           [
            {size, 10},
            {max_overflow, 30}
           ],
           [
            {database, <<"rabbitmq_logs">>},
            {w_mode, safe}
           ]}
         }
        ]},
  {applications, [kernel, stdlib, rabbit, mnesia, bson, crypto, mongodb, mongodb_pool]}]}.
